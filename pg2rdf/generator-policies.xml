<?xml version="1.0" encoding="UTF-8"?>
<generator_policy>

    <generator name="namedgraph" prefix="custom">
	    <pattern>{type}/{id}/graph</pattern>
    </generator>

    <generator name="Label" type="label">
        <pattern>{label}</pattern>
    </generator>

    <generator name="URIwithType" prefix="custom">
	    <pattern>{type}/{id}</pattern>
    </generator>
    
    <generator name="URIwithTypeHashed" prefix="custom" shorten="yes">
        <pattern>{type}/{id}</pattern>
    </generator>

    <generator name="URIorUUID">
        <custom generatorClass="gr.forth.URIorUUID">
            <set-arg name="text"></set-arg>
        </custom>
    </generator>

</generator_policy>